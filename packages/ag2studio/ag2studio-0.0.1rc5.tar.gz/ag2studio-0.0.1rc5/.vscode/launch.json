{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "AG2Studio Debug",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "ag2studio.web.app:app",
                "--host",
                "127.0.0.1",
                "--port",
                "8081",
                "--reload"
            ],
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "AG2STUDIO_API_DOCS": "false"
            }
        }
    ]
}