{
    "name": "additional_difficulty",
    "version": "0.0.2",
    "description": "Meta-calculator. Quantifies difficulty of mental arithmetic.  Additionally supports subtraction, multiplication and long-division.",
    "main": "./dist/index.mjs",
    "types": "./dist/index.d.mts",
    "directories": {
        "test": "tests"
    },
    "scripts": {
        "prepublish": "tsc",
        "typecheck": "tsc --noEmit",
        "eslint": "eslint . --cache",
        "prettier": "prettier --check \"**/*.{js,msx,mjs,ts,tsx,mts,mtx}\"",
        "prettier:write": "prettier --write \"**/*.{js,msx,mjs,ts,tsx,mts,mtx}\""
    },
    "files": [
        "./dist/index.mjs",
        "./dist/index.d.mts"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NumberzGame/additional_difficulty.git"
    },
    "keywords": [
        "Heuristic",
        "Arithmetic"
    ],
    "author": "James Parrott <james.parrott@proton.me>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/NumberzGame/additional_difficulty/issues"
    },
    "homepage": "https://github.com/NumberzGame/additional_difficulty#readme",
    "devDependencies": {
        "@eslint/js": "^9.18.0",
        "@types/node": "^22.10.5",
        "eslint": "^9.18.0",
        "fast-check": "^3.22.0",
        "globals": "^15.11.0",
        "prettier": "3.3.3",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.20.0"
    },
    "overrides": {
        "glob": "^11.0.0"
    }
}
