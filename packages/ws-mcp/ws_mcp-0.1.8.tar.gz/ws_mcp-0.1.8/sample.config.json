{
  "mcpServers": {
    "wcgw": {
      "command": "uvx",
      "args": [
        "--refresh",
        "--from",
        "wcgw-linux-compatible-fork@latest",
        "--python",
        "3.12",
        "wcgw_mcp"
      ]
    },
    "fetch": {
      "command": "uvx",
      "args": [
        "--refresh",
        "mcp-server-fetch"
      ]
    }
  }
}
