{
  "coding_mode": "inductive",
  "use_parsing": true,
  "preliminary_segments_per_prompt": 50,
  "meaning_units_per_assignment_prompt": 10,
  "context_size": 5,

  "embedding_model": "text-embedding-3-small",

  "data_format": "transcript",
  "paths": {
    "prompts_folder": "prompts",
    "codebase_folder": "qual_codebase",
    "json_folder": "json_inputs",
    "config_folder": "configs"
  },
  "selected_codebase": "teacher_schema.jsonl",
  "selected_json_file": "teacher_transcript.json",
  "parse_prompt_file": "parse.txt",
  "inductive_coding_prompt_file": "inductive.txt",
  "deductive_coding_prompt_file": "deductive.txt",
  "output_folder": "outputs",

  "enable_logging": true,
  "logging_level": "INFO",
  "log_to_file": true,
  "log_file_path": "logs/application.log",

  "thread_count": 5,

  "parse_llm_config": {
    "provider": "openai",
    "model_name": "gpt-4o-mini",
    "temperature": 0.7,
    "max_tokens": 2000,
    "api_key": ""
  },
  "assign_llm_config": {
    "provider": "openai",
    "model_name": "gpt-4o-mini",
    "temperature": 0.7,
    "max_tokens": 2000,
    "api_key": ""
  }
