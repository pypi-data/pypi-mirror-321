<object class="DozorParallelProcessing">
  <object href="/mxcollect" role="collect"/>
  <object href="/mini-diff" role="diffractometer"/>
  <object href="/eh2/diff-beamstop" role="beamstop"/>
  <processing_command>/opt/embl-hh/bin/edna_dozor.sh</processing_command>

  <grid_properties>[{"grid_num_col": 6, "grid_num_row": 6, "comp_num_row": 24, "comp_num_col": 24, "inverted_rows": True},
                    {"grid_num_col": 8, "grid_num_row": 8, "comp_num_row": 20, "comp_num_col": 20, "inverted_rows": True}]</grid_properties>
</object>


